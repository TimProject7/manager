<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.parker.dao.AdminDAO">

	<select id="loginCheck" parameterType="com.parker.vo.AdminVO" resultType="com.parker.vo.AdminVO">
		SELECT admin_id, admin_number
		FROM admin_tb
		WHERE
		admin_id = #{admin_id} AND
		admin_password =
		#{admin_password}
	</select>
</mapper>
